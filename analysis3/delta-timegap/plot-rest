

set title sprintf("CheckInterval 1 Range 15\n Range and tracking gaps")
set xlabel "Delta (seconds)" 
set ylabel "Highest tracking gap (minutes)"

set grid
set boxwidth 0.9 relative
set style data histograms
set style histogram cluster
set style fill solid 1.0 border lt -1
set xtics (15,30,45)
set yrange [0:25]

set terminal png truecolor font small size 600,500
set output 'triangulation-CI-1-R15.png'

plot 'triangulation-CI-1-R15-PCT10-summary.csv' using 2:xticlabels(1) title "average"

reset

